<?xml version="1.0" encoding="UTF-8"?>
<root>
	<!-- 
	This is the configuration file for fa-commons-localization.jar package.
	 -->
	 
	<!-- 
	Change localhost to the hostname where the services are actually deployed.
	Parameter is required. 
	-->
	<corews>http://localhost:9085/corews/corews</corews>
	
	<!-- 
	Sets the time to live for an element before it expires.
    	i.e. The maximum time between creation time and when an element expires.
    	Parameter is optional. The default value is 600 sec.
           -->
	<timeToLiveSeconds>600</timeToLiveSeconds>
</root>
