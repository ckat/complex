<?xml version="1.0" encoding="utf-8"?>
<data-map xmlns="http://cayenne.apache.org/schema/3.0/modelMap"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://cayenne.apache.org/schema/3.0/modelMap http://cayenne.apache.org/schema/3.0/modelMap.xsd"
  project-version="3.0.0.1">
	<property name="defaultPackage" value="com.homework.hr.entities"/>
	<property name="defaultSchema" value="HR"/>
	<db-entity name="COUNTRIES" schema="HR">
		<db-attribute name="COUNTRY_ID" type="CHAR" isPrimaryKey="true" isMandatory="true" length="2"/>
		<db-attribute name="COUNTRY_NAME" type="VARCHAR" length="40"/>
		<db-attribute name="REGION_ID" type="INTEGER"/>
	</db-entity>
	<db-entity name="DEPARTMENTS" schema="HR">
		<db-attribute name="DEPARTMENT_ID" type="INTEGER" isPrimaryKey="true" isMandatory="true" length="4"/>
		<db-attribute name="DEPARTMENT_NAME" type="VARCHAR" isMandatory="true" length="30"/>
		<db-attribute name="LOCATION_ID" type="INTEGER" length="4"/>
		<db-attribute name="MANAGER_ID" type="INTEGER" length="6"/>
	</db-entity>
	<db-entity name="EMPLOYEES" schema="HR">
		<db-attribute name="COMMISSION_PCT" type="DECIMAL" length="2" scale="2"/>
		<db-attribute name="DEPARTMENT_ID" type="INTEGER" length="4"/>
		<db-attribute name="EMAIL" type="VARCHAR" isMandatory="true" length="25"/>
		<db-attribute name="EMPLOYEE_ID" type="INTEGER" isPrimaryKey="true" isMandatory="true" length="6"/>
		<db-attribute name="FIRST_NAME" type="VARCHAR" length="20"/>
		<db-attribute name="HIRE_DATE" type="TIMESTAMP" isMandatory="true" length="7"/>
		<db-attribute name="JOB_ID" type="VARCHAR" isMandatory="true" length="10"/>
		<db-attribute name="LAST_NAME" type="VARCHAR" isMandatory="true" length="25"/>
		<db-attribute name="MANAGER_ID" type="INTEGER" length="6"/>
		<db-attribute name="PHONE_NUMBER" type="VARCHAR" length="20"/>
		<db-attribute name="SALARY" type="DECIMAL" length="8" scale="2"/>
	</db-entity>
	<db-entity name="EMP_DETAILS_VIEW" schema="HR">
		<db-attribute name="CITY" type="VARCHAR" isMandatory="true" length="30"/>
		<db-attribute name="COMMISSION_PCT" type="DECIMAL" length="2" scale="2"/>
		<db-attribute name="COUNTRY_ID" type="CHAR" length="2"/>
		<db-attribute name="COUNTRY_NAME" type="VARCHAR" length="40"/>
		<db-attribute name="DEPARTMENT_ID" type="INTEGER" length="4"/>
		<db-attribute name="DEPARTMENT_NAME" type="VARCHAR" isMandatory="true" length="30"/>
		<db-attribute name="EMPLOYEE_ID" type="INTEGER" isPrimaryKey="true" isMandatory="true" length="6"/>
		<db-attribute name="FIRST_NAME" type="VARCHAR" length="20"/>
		<db-attribute name="JOB_ID" type="VARCHAR" isMandatory="true" length="10"/>
		<db-attribute name="JOB_TITLE" type="VARCHAR" isMandatory="true" length="35"/>
		<db-attribute name="LAST_NAME" type="VARCHAR" isMandatory="true" length="25"/>
		<db-attribute name="LOCATION_ID" type="INTEGER" length="4"/>
		<db-attribute name="MANAGER_ID" type="INTEGER" length="6"/>
		<db-attribute name="REGION_NAME" type="VARCHAR" length="25"/>
		<db-attribute name="SALARY" type="DECIMAL" length="8" scale="2"/>
		<db-attribute name="STATE_PROVINCE" type="VARCHAR" length="25"/>
	</db-entity>
	<db-entity name="JOBS" schema="HR">
		<db-attribute name="JOB_ID" type="VARCHAR" isPrimaryKey="true" isMandatory="true" length="10"/>
		<db-attribute name="JOB_TITLE" type="VARCHAR" isMandatory="true" length="35"/>
		<db-attribute name="MAX_SALARY" type="INTEGER" length="6"/>
		<db-attribute name="MIN_SALARY" type="INTEGER" length="6"/>
	</db-entity>
	<db-entity name="JOB_HISTORY" schema="HR">
		<db-attribute name="DEPARTMENT_ID" type="INTEGER" length="4"/>
		<db-attribute name="EMPLOYEE_ID" type="INTEGER" isPrimaryKey="true" isMandatory="true" length="6"/>
		<db-attribute name="END_DATE" type="TIMESTAMP" isMandatory="true" length="7"/>
		<db-attribute name="JOB_ID" type="VARCHAR" isMandatory="true" length="10"/>
		<db-attribute name="START_DATE" type="TIMESTAMP" isPrimaryKey="true" isMandatory="true" length="7"/>
	</db-entity>
	<db-entity name="LOCATIONS" schema="HR">
		<db-attribute name="CITY" type="VARCHAR" isMandatory="true" length="30"/>
		<db-attribute name="COUNTRY_ID" type="CHAR" length="2"/>
		<db-attribute name="LOCATION_ID" type="INTEGER" isPrimaryKey="true" isMandatory="true" length="4"/>
		<db-attribute name="POSTAL_CODE" type="VARCHAR" length="12"/>
		<db-attribute name="STATE_PROVINCE" type="VARCHAR" length="25"/>
		<db-attribute name="STREET_ADDRESS" type="VARCHAR" length="40"/>
	</db-entity>
	<db-entity name="REGIONS" schema="HR">
		<db-attribute name="REGION_ID" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="REGION_NAME" type="VARCHAR" length="25"/>
	</db-entity>
	<obj-entity name="Countries" className="com.homework.hr.entities.Countries" dbEntityName="COUNTRIES">
		<obj-attribute name="countryName" type="java.lang.String" db-attribute-path="COUNTRY_NAME"/>
	</obj-entity>
	<obj-entity name="Departments" className="com.homework.hr.entities.Departments" dbEntityName="DEPARTMENTS">
		<obj-attribute name="departmentName" type="java.lang.String" db-attribute-path="DEPARTMENT_NAME"/>
	</obj-entity>
	<obj-entity name="EmpDetailsView" className="com.homework.hr.entities.EmpDetailsView" dbEntityName="EMP_DETAILS_VIEW">
		<obj-attribute name="city" type="java.lang.String" db-attribute-path="CITY"/>
		<obj-attribute name="commissionPct" type="java.math.BigDecimal" db-attribute-path="COMMISSION_PCT"/>
		<obj-attribute name="countryId" type="java.lang.String" db-attribute-path="COUNTRY_ID"/>
		<obj-attribute name="countryName" type="java.lang.String" db-attribute-path="COUNTRY_NAME"/>
		<obj-attribute name="departmentId" type="java.lang.Integer" db-attribute-path="DEPARTMENT_ID"/>
		<obj-attribute name="departmentName" type="java.lang.String" db-attribute-path="DEPARTMENT_NAME"/>
		<obj-attribute name="employeeId" type="java.lang.Integer" db-attribute-path="EMPLOYEE_ID"/>
		<obj-attribute name="firstName" type="java.lang.String" db-attribute-path="FIRST_NAME"/>
		<obj-attribute name="jobId" type="java.lang.String" db-attribute-path="JOB_ID"/>
		<obj-attribute name="jobTitle" type="java.lang.String" db-attribute-path="JOB_TITLE"/>
		<obj-attribute name="lastName" type="java.lang.String" db-attribute-path="LAST_NAME"/>
		<obj-attribute name="locationId" type="java.lang.Integer" db-attribute-path="LOCATION_ID"/>
		<obj-attribute name="managerId" type="java.lang.Integer" db-attribute-path="MANAGER_ID"/>
		<obj-attribute name="regionName" type="java.lang.String" db-attribute-path="REGION_NAME"/>
		<obj-attribute name="salary" type="java.math.BigDecimal" db-attribute-path="SALARY"/>
		<obj-attribute name="stateProvince" type="java.lang.String" db-attribute-path="STATE_PROVINCE"/>
	</obj-entity>
	<obj-entity name="Employees" className="com.homework.hr.entities.Employees" dbEntityName="EMPLOYEES">
		<obj-attribute name="commissionPct" type="java.math.BigDecimal" db-attribute-path="COMMISSION_PCT"/>
		<obj-attribute name="email" type="java.lang.String" db-attribute-path="EMAIL"/>
		<obj-attribute name="firstName" type="java.lang.String" db-attribute-path="FIRST_NAME"/>
		<obj-attribute name="hireDate" type="java.util.Date" db-attribute-path="HIRE_DATE"/>
		<obj-attribute name="lastName" type="java.lang.String" db-attribute-path="LAST_NAME"/>
		<obj-attribute name="phoneNumber" type="java.lang.String" db-attribute-path="PHONE_NUMBER"/>
		<obj-attribute name="salary" type="java.math.BigDecimal" db-attribute-path="SALARY"/>
	</obj-entity>
	<obj-entity name="JobHistory" className="com.homework.hr.entities.JobHistory" dbEntityName="JOB_HISTORY">
		<obj-attribute name="endDate" type="java.util.Date" db-attribute-path="END_DATE"/>
	</obj-entity>
	<obj-entity name="Jobs" className="com.homework.hr.entities.Jobs" dbEntityName="JOBS">
		<obj-attribute name="jobTitle" type="java.lang.String" db-attribute-path="JOB_TITLE"/>
		<obj-attribute name="maxSalary" type="java.lang.Integer" db-attribute-path="MAX_SALARY"/>
		<obj-attribute name="minSalary" type="java.lang.Integer" db-attribute-path="MIN_SALARY"/>
	</obj-entity>
	<obj-entity name="Locations" className="com.homework.hr.entities.Locations" dbEntityName="LOCATIONS">
		<obj-attribute name="city" type="java.lang.String" db-attribute-path="CITY"/>
		<obj-attribute name="postalCode" type="java.lang.String" db-attribute-path="POSTAL_CODE"/>
		<obj-attribute name="stateProvince" type="java.lang.String" db-attribute-path="STATE_PROVINCE"/>
		<obj-attribute name="streetAddress" type="java.lang.String" db-attribute-path="STREET_ADDRESS"/>
	</obj-entity>
	<obj-entity name="Regions" className="com.homework.hr.entities.Regions" dbEntityName="REGIONS">
		<obj-attribute name="regionName" type="java.lang.String" db-attribute-path="REGION_NAME"/>
	</obj-entity>
	<db-relationship name="locationsArray" source="COUNTRIES" target="LOCATIONS" toMany="true">
		<db-attribute-pair source="COUNTRY_ID" target="COUNTRY_ID"/>
	</db-relationship>
	<db-relationship name="toRegions" source="COUNTRIES" target="REGIONS" toMany="false">
		<db-attribute-pair source="REGION_ID" target="REGION_ID"/>
	</db-relationship>
	<db-relationship name="employeesArray" source="DEPARTMENTS" target="EMPLOYEES" toMany="true">
		<db-attribute-pair source="DEPARTMENT_ID" target="DEPARTMENT_ID"/>
	</db-relationship>
	<db-relationship name="jobHistoryArray" source="DEPARTMENTS" target="JOB_HISTORY" toMany="true">
		<db-attribute-pair source="DEPARTMENT_ID" target="DEPARTMENT_ID"/>
	</db-relationship>
	<db-relationship name="toEmployees" source="DEPARTMENTS" target="EMPLOYEES" toMany="false">
		<db-attribute-pair source="MANAGER_ID" target="EMPLOYEE_ID"/>
	</db-relationship>
	<db-relationship name="toLocations" source="DEPARTMENTS" target="LOCATIONS" toMany="false">
		<db-attribute-pair source="LOCATION_ID" target="LOCATION_ID"/>
	</db-relationship>
	<db-relationship name="departmentsArray" source="EMPLOYEES" target="DEPARTMENTS" toMany="true">
		<db-attribute-pair source="EMPLOYEE_ID" target="MANAGER_ID"/>
	</db-relationship>
	<db-relationship name="employeesArray" source="EMPLOYEES" target="EMPLOYEES" toMany="true">
		<db-attribute-pair source="EMPLOYEE_ID" target="MANAGER_ID"/>
	</db-relationship>
	<db-relationship name="jobHistoryArray" source="EMPLOYEES" target="JOB_HISTORY" toDependentPK="true" toMany="true">
		<db-attribute-pair source="EMPLOYEE_ID" target="EMPLOYEE_ID"/>
	</db-relationship>
	<db-relationship name="toDepartments" source="EMPLOYEES" target="DEPARTMENTS" toMany="false">
		<db-attribute-pair source="DEPARTMENT_ID" target="DEPARTMENT_ID"/>
	</db-relationship>
	<db-relationship name="toEmployees" source="EMPLOYEES" target="EMPLOYEES" toMany="false">
		<db-attribute-pair source="MANAGER_ID" target="EMPLOYEE_ID"/>
	</db-relationship>
	<db-relationship name="toJobs" source="EMPLOYEES" target="JOBS" toMany="false">
		<db-attribute-pair source="JOB_ID" target="JOB_ID"/>
	</db-relationship>
	<db-relationship name="employeesArray" source="JOBS" target="EMPLOYEES" toMany="true">
		<db-attribute-pair source="JOB_ID" target="JOB_ID"/>
	</db-relationship>
	<db-relationship name="jobHistoryArray" source="JOBS" target="JOB_HISTORY" toMany="true">
		<db-attribute-pair source="JOB_ID" target="JOB_ID"/>
	</db-relationship>
	<db-relationship name="toDepartments" source="JOB_HISTORY" target="DEPARTMENTS" toMany="false">
		<db-attribute-pair source="DEPARTMENT_ID" target="DEPARTMENT_ID"/>
	</db-relationship>
	<db-relationship name="toEmployees" source="JOB_HISTORY" target="EMPLOYEES" toMany="false">
		<db-attribute-pair source="EMPLOYEE_ID" target="EMPLOYEE_ID"/>
	</db-relationship>
	<db-relationship name="toJobs" source="JOB_HISTORY" target="JOBS" toMany="false">
		<db-attribute-pair source="JOB_ID" target="JOB_ID"/>
	</db-relationship>
	<db-relationship name="departmentsArray" source="LOCATIONS" target="DEPARTMENTS" toMany="true">
		<db-attribute-pair source="LOCATION_ID" target="LOCATION_ID"/>
	</db-relationship>
	<db-relationship name="toCountries" source="LOCATIONS" target="COUNTRIES" toMany="false">
		<db-attribute-pair source="COUNTRY_ID" target="COUNTRY_ID"/>
	</db-relationship>
	<db-relationship name="countriesArray" source="REGIONS" target="COUNTRIES" toMany="true">
		<db-attribute-pair source="REGION_ID" target="REGION_ID"/>
	</db-relationship>
	<obj-relationship name="locationsArray" source="Countries" target="Locations" deleteRule="Deny" db-relationship-path="locationsArray"/>
	<obj-relationship name="toRegions" source="Countries" target="Regions" deleteRule="Nullify" db-relationship-path="toRegions"/>
	<obj-relationship name="employeesArray" source="Departments" target="Employees" deleteRule="Deny" db-relationship-path="employeesArray"/>
	<obj-relationship name="jobHistoryArray" source="Departments" target="JobHistory" deleteRule="Deny" db-relationship-path="jobHistoryArray"/>
	<obj-relationship name="toEmployees" source="Departments" target="Employees" deleteRule="Nullify" db-relationship-path="toEmployees"/>
	<obj-relationship name="toLocations" source="Departments" target="Locations" deleteRule="Nullify" db-relationship-path="toLocations"/>
	<obj-relationship name="departmentsArray" source="Employees" target="Departments" deleteRule="Deny" db-relationship-path="departmentsArray"/>
	<obj-relationship name="employeesArray" source="Employees" target="Employees" deleteRule="Deny" db-relationship-path="employeesArray"/>
	<obj-relationship name="jobHistoryArray" source="Employees" target="JobHistory" deleteRule="Deny" db-relationship-path="jobHistoryArray"/>
	<obj-relationship name="toDepartments" source="Employees" target="Departments" deleteRule="Nullify" db-relationship-path="toDepartments"/>
	<obj-relationship name="toEmployees" source="Employees" target="Employees" deleteRule="Nullify" db-relationship-path="toEmployees"/>
	<obj-relationship name="toJobs" source="Employees" target="Jobs" deleteRule="Nullify" db-relationship-path="toJobs"/>
	<obj-relationship name="toDepartments" source="JobHistory" target="Departments" deleteRule="Nullify" db-relationship-path="toDepartments"/>
	<obj-relationship name="toEmployees" source="JobHistory" target="Employees" deleteRule="Nullify" db-relationship-path="toEmployees"/>
	<obj-relationship name="toJobs" source="JobHistory" target="Jobs" deleteRule="Nullify" db-relationship-path="toJobs"/>
	<obj-relationship name="employeesArray" source="Jobs" target="Employees" deleteRule="Deny" db-relationship-path="employeesArray"/>
	<obj-relationship name="jobHistoryArray" source="Jobs" target="JobHistory" deleteRule="Deny" db-relationship-path="jobHistoryArray"/>
	<obj-relationship name="departmentsArray" source="Locations" target="Departments" deleteRule="Deny" db-relationship-path="departmentsArray"/>
	<obj-relationship name="toCountries" source="Locations" target="Countries" deleteRule="Nullify" db-relationship-path="toCountries"/>
	<obj-relationship name="countriesArray" source="Regions" target="Countries" deleteRule="Deny" db-relationship-path="countriesArray"/>
	<query name="EmployeesQuery" factory="org.apache.cayenne.map.SelectQueryBuilder" root="obj-entity" root-name="Employees">
		<property name="cayenne.GenericSelectQuery.fetchingDataRows" value="true"/>
	</query>
</data-map>
